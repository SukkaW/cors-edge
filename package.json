{
  "name": "cors-edge",
  "version": "1.0.1",
  "description": "CORS utility function for Cloudflare Workers, Fastly Edge Compute, Deno Deploy, Vercel Edge Runtime",
  "repository": {
    "url": "https://github.com/SukkaW/cors-edge"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --format=sukka .",
    "test": "SWC_NODE_IGNORE_DYNAMIC=1 SWC_NODE_INLINE_SOURCE_MAP=1 mocha --require @swc-node/register --parallel --full-trace ./src/**/*.test.ts",
    "build": "rollup -c rollup.config.ts --configPlugin swc3 --bundleConfigAsCjs",
    "prerelease": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "bumpp -r --all --commit \"release: %s\" --tag \"%s\""
  },
  "keywords": [
    "cors",
    "cloudflare workers",
    "fastly edge compute",
    "deno deploy",
    "vercel edge runtime"
  ],
  "author": "SukkaW <https://skk.moe>",
  "license": "MIT",
  "dependencies": {
    "rollup": "^4.53.3",
    "rollup-plugin-swc3": "^0.12.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.3",
    "@types/mocha": "^10.0.10",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "eslint-config-sukka": "^8.0.5",
    "eslint-formatter-sukka": "^8.0.5",
    "expect": "^30.2.0",
    "mocha": "^11.7.5",
    "rollup-plugin-dts": "^6.3.0",
    "typescript": "^5.9.3",
    "xbits": "^0.2.0"
  },
  "packageManager": "pnpm@10.25.0"
}
